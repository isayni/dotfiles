configuration {
    modes: "window,run,drun,ssh,combi";
    font: "JetBrains Mono Semibold 12";
    show-icons: true;
    terminal: "alacritty";
/*	ssh-command: "{terminal} -e {ssh-client} {host} [-p {port}]";*/
    icon-theme: "Papirus";
    combi-modes: "run";
    combi-hide-mode-prefix: true;
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
}
@theme "/usr/share/rofi/themes/gruvbox-dark-hard.rasi"
